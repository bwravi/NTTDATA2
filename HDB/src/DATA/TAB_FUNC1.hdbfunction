FUNCTION "PROJ4::TAB_FUNC1"(IN MAT VARCHAR(10) )
       RETURNS TABLE(ORDID	NVARCHAR(10),
ORDER_DATE	DATE,
REQ_DATE	DATE,
CUSTID	NVARCHAR(10),
MATID	NVARCHAR(18),
EMPID	NVARCHAR(10),
ORDER_QTY	DECIMAL(17,2),
UNIT	VARCHAR(3),
PRICE	DECIMAL(17,2),
CURRENCY	VARCHAR(5),
YR	NVARCHAR(4)
) 
       LANGUAGE SQLSCRIPT 
       SQL SECURITY INVOKER AS 
BEGIN 
VAR1 = SELECT * FROM "PROJ4::ORDERS" WHERE MATID = :MAT;
RETURN :VAR1;
END;