PROCEDURE "PROJ4::PROC2"(IN MAT VARCHAR(10),OUT TAB TABLE(ORDID	NVARCHAR(10),
ORDER_DATE	DATE,
REQ_DATE	DATE,
CUSTID	NVARCHAR(10),
MATID	NVARCHAR(18),
EMPID	NVARCHAR(10),
ORDER_QTY	DECIMAL(17,2),
UNIT	VARCHAR(3),
PRICE	DECIMAL(17,2),
CURRENCY	VARCHAR(5),
YR	NVARCHAR(4)
) )
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER
   --DEFAULT SCHEMA <default_schema_name>
   READS SQL DATA AS
BEGIN
TAB = SELECT * FROM "PROJ4::ORDERS" WHERE MATID = :MAT;
END